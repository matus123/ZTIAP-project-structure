{
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "allowJs": true,
        "esModuleInterop": true,
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictPropertyInitialization": true,
        "lib": [
            "es2017",
            "dom"
        ],
    }
}
  